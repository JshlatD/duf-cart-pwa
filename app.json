    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro",
      "display": "standalone",
      "themeColor": "#111827",
      "backgroundColor": "#f4f6f8",
      "description": "DUF Quick Order System"
    },
    "plugins": ["expo-router"]
  }
